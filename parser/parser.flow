testAeSmp() -> void{
    println("\nTEST: AeSmp");
    t1 = either(parse("(1 + ((((1 * (d + b)) - (e * (1 + 1))) / ((d + b) * (d + b))) - 1))"), ArInt(0));
    println("   raw ArExp:");
    println(t1);
    println("   ae2s(ArExp):");
    println(ae2s(t1));
    t2 = aeSmp(t1);
    println("   ae2s(aeSmp(ArExp)):");
    println2(ae2s(t2));
}

printAeRatTest(t1 : ArExp){
    println("   raw ArExp:");
    println(t1);
    println("   ae2s(ArExp):");
    println(ae2s(t1));
    t2 = alg2rat(t1);
    println("   rat2s(alg2rat(ArExp)):");
    println(rat2s(t2));
}

testAeRat(){
    println("\nTEST: AeRat");
    t1 = either(parse(  "( (z / (1 + (1 / (x + z)) )) + (y + ((x + 1)/(x - 1))) )"  ), ArInt(0));
    printAeRatTest(t1);
    t2 = either(parse(  "( ((2*a)/( (4 * (a * a)) -  (b*b))) + (1/(b - (2 * a))) )"  ), ArInt(0));
    printAeRatTest(t2);
}

